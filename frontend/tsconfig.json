{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "frontend/.expo/types/**/*.ts",
    "expo-env.d.ts",
    ".expo/types/**/*.ts"
, "app/(tabs)/index.jsx"  ]
}

